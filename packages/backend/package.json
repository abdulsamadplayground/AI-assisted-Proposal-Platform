{
  "name": "@ai-proposal-platform/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js dotenv_config_path=.env.production",
    "vercel-build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "migrate:latest": "knex migrate:latest --knexfile src/db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/db/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/db/knexfile.ts",
    "seed:run": "knex seed:run --knexfile src/db/knexfile.ts"
  },
  "dependencies": {
    "express": "^4.18.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "sqlite3": "^5.1.7",
    "zod": "^3.22.4",
    "multer": "^2.0.0-rc.4",
    "aws-sdk": "^2.1585.0",
    "uuid": "^9.0.1",
    "docx": "^9.5.0",
    "diff": "^8.0.0",
    "winston": "^3.11.0",
    "dotenv": "^16.4.5",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.8",
    "@types/node": "^20.12.7",
    "typescript": "^5.3.3",
    "tsx": "^4.7.2",
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@types/jest": "^29.5.12",
    "fast-check": "^4.5.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
